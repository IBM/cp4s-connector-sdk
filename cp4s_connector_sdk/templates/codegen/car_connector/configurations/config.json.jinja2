{
    "connection": {
        "type": {
            "default": "{{package_name}}",
            "extentionType": "car"
        },
        "name": {
            "min": 5,
            "type": "text",
            "regex": "^[A-Za-z0-9 _-]*$",
            "default": null
        },
        "description": {
            "type": "text",
            "regex": "^[A-Za-z0-9 _-]*$",
            "default": null
        },
        "frequency": {
            "default": "15m",
            "type": "dropdown",
            "options": ["5m", "10m", "15m", "30m", "1h","2h","4h","6h","12h", "daily"]
        },
        "time": {
            "default": "09:00",
            "type": "time"
        },
        "help": {
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.4.0/platform/docs/scp-core/data-sources-car.html",
            "type": "link"
        },
        "image": {
            "type": "text",
            "hidden": true,
            "default": "{image_link}"
        }
    },
    "configuration": {
        "auth": {
            "username": {
                "type": "text"
            },
            "password": {
                "type": "password"
            }
        },
        "parameter":{
        	"<required_parameter>": {
                "type": "text"
            },
            "<optional_parameter>": {
                "type": "text",
                "optional": true
            }
        }
    }
}